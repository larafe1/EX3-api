{
  "name": "ex3-api",
  "version": "0.0.0",
  "main": "server.js",
  "repository": "https://github.com/larafe1/EX3-api.git",
  "author": "larafe1",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/server.js",
    "dev": "tsnd --respawn --transpile-only --exit-child -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json&&tscpaths -p tsconfig.json -s src -o dist",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --check \"src/**/*.ts\" --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed:users": "tsnd src/database/seed/users.ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.12.0",
    "uuid": "^8.3.2"
  }
}
